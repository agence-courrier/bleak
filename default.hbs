<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

	<title>{{meta_title}}</title>

	<meta name="HandheldFriendly" content="True" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />

	<link rel="stylesheet" type="text/css" href="{{asset "css/dependencies.css"}}" />
	<link rel="stylesheet" type="text/css" href="{{asset "css/style.css"}}" />

	{{ghost_head}}
</head>

<body class="{{body_class}}">
	<div id="wrapper">
		<div id="ajax-container">
			<nav id="nav" class="nav">
				<div class="nav-logo">
					<a href="{{@site.url}}">
						{{#if @site.logo}}
						<img src="{{@site.logo}}" alt="{{t "Logo" }}" />
						{{/if}}
					</a>
				

					 <a href="http://localhost:2368/tag/getting-started/" class="navbar-item">Services</a>
                     <a href="/" class="navbar-item">Contact</a>
				</div>
				<div class="nav-menu">
			   {{#if @site.facebook}}
			    <a class="facebook" href="{{facebook_url}}" title="{{@site.facebook}}"><i class="icon-facebook"></i></a>
			   {{/if}}
			   <a class="social-link social-link-ig fa-instagram" href="https://www.instagram.com/ghost/" title="Instagram" target="_blank" rel="noopener">{{> "icons/instagram"}}</a>
			 <a class="rss" href="{{@site.url}}/rss/"><i class="icon-rss"></i></a>
				</div>
			</nav>
			{{{body}}}
			<div id="body-class" style="display: none;" class="{{body_class}}"></div>
			<footer id="footer">
				<section class="credits">
					
				</section>
			</footer>
			{{#if @site.navigation}}
			{{navigation}}
			{{/if}}
		</div>
	</div>
	{{ghost_foot}}

	<script type="text/javascript" src="{{asset "js/dependencies.js"}}"></script>
	<script type="text/javascript" src="{{asset "js/script.js"}}"></script>
</body>
</html>
